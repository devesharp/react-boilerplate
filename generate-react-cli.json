{
   "usesTypeScript": true,
   "usesCssModule": false,
   "cssPreprocessor": "scss",
   "testLibrary": "Testing Library",
   "component": {
      "default": {
         "path": "src/app/components",
         "withStory": false,
         "withLazy": true,
         "withStyle": true,
         "withTest": false,
         "customTemplates": {
            "index": "templates/components/index.tsx",
            "component": "templates/components/TemplateName.tsx",
            "style": "templates/components/TemplateName.style.tsx",
            "lazy": "templates/components/TemplateName.interface.tsx"
         }
      },
      "page": {
         "path": "src/app/pages",
         "withStory": true,
         "withLazy": true,
         "withStyle": true,
         "withTest": false,
         "customTemplates": {
            "index": "templates/pages/index.tsx",
            "component": "templates/pages/TemplateNamePage.tsx",
            "style": "templates/pages/TemplateNamePage.style.tsx",
            "story": "templates/pages/TemplateNamePage.controller.tsx",
            "lazy": "templates/pages/TemplateNamePage.interface.tsx"
         }
      }
   }
}